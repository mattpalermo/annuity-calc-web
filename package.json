{
  "name": "annuity-calc-web",
  "version": "0.0.0",
  "description": "Simple annuity calculator web app",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node ./src/server.js",
    "scss": "node-sass --output-style compressed -o dist/styles src/styles",
    "webpack": "webpack --config webpack.config.js",
    "build": "npm run scss && npm run webpack"
  },
  "keywords": [
    "annuity",
    "calculator",
    "web",
    "app"
  ],
  "author": "Matthew Palermo",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "node-sass": "^4.1.1",
    "webpack": "^1.14.0"
  }
}
